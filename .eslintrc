{
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "es6": true
    },
    "extends": "airbnb",
    "rules": {
        "linebreak-style": 0,
        "no-tabs": 0,
        "comma-dangle": 0,
        "import/prefer-default-export": 0,
        "indent": ["error", 2, {
            "SwitchCase": 1
        }],
        "no-use-before-define": 0,
        "class-methods-use-this": 0
    },
    "globals": {
    }
}
